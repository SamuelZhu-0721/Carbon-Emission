{
  "name": "vite-plugin-cesium",
  "version": "1.2.22",
  "description": "Cesium library plugin for Vite",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
