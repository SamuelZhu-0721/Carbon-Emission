  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "prepare": "npm run build"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "cesium"
  ],
  "author": "nshen <nshen121@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nshen/vite-plugin-cesium"
  },
  "bugs": {
    "url": "https://github.com/nshen/vite-plugin-cesium/issues"
  },
  "dependencies": {
    "fs-extra": "^9.1.0",
    "rollup-plugin-external-globals": "^0.6.1",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.7",
    "@types/fs-extra": "^9.0.8",
    "@types/node": "^14.14.31",
    "@types/serve-static": "^1.13.9",
    "prettier": "^2.2.1",
    "tsup": "^6.2.1",
    "typescript": "^4.2.2",
    "vite": "^3.0.4"
  },
  "peerDependencies": {
    "cesium": "^1.95.0",
    "vite": ">=2.7.1"
  }
